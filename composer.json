{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7",
        "doctrine",
        "docker"
    ],
    "homepage": "https://github.com/SyntaxErrorLineNULL/test-rest-api",
    "license": "MIT",
    "authors": [
        {
            "name": "SyntaxErrorLineNULL",
            "email": "cyberorange16@gmail.com",
            "homepage": "https://github.com/SyntaxErrorLineNULL/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/migrations": "^3.1",
        "doctrine/orm": "^2.8",
        "firebase/php-jwt": "^5.2",
        "guzzlehttp/psr7": "^1.8",
        "http-interop/http-factory-guzzle": "^1.0",
        "jms/serializer": "^3.12",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-diactoros": "^2.5",
        "monolog/monolog": "^2.2",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "php-di/php-di": "^6.3",
        "pimple/pimple": "^3.4",
        "ramsey/uuid": "^4.1",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "symfony/console": "^5.0",
        "symfony/property-access": "^5.2",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^5.2",
        "vlucas/phpdotenv": "^5.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
